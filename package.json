{
  "name": "fetchq-cron",
  "version": "1.0.0",
  "description": "",
  "main": "ssr/index.js",
  "scripts": {
    "### Development Utilities": "",
    "start": "node ssr/index.js",
    "startdev:": "yarn && ./node_modules/.bin/npm-run-all --parallel start:dev:*",
    "start:dev:api": "nodemon ssr/index.js",
    "start:dev:app": "react-scripts start",
    "start:docker:api": "yarn && nodemon ssr/index.js",
    "start:docker:app": "yarn && react-scripts start",
    "### Testing Utilities": "",
    "test:unit": "./node_modules/.bin/jest --projects test/unit",
    "test:e2e": "./node_modules/.bin/jest --projects test/e2e --runInBand --testTimeout=10000",
    "tdd:e2e": "./node_modules/.bin/jest --projects test/e2e --runInBand --watchAll --testTimeout=10000",
    "tdd:unit": "./node_modules/.bin/jest --projects test/unit --watchAll",
    "### Production Utilities": "",
    "build": "react-scripts build"
  },
  "dependencies": {
    "npm-run-all": "4.1.5",
    "@forrestjs/hooks": "3.9.0",
    "axios": "0.19.2",
    "cron-parser": "2.13.0",
    "envalid": "6.0.0",
    "fastify": "2.11.0",
    "fastify-cors": "3.0.1",
    "fetch-resolver": "1.3.0",
    "fetchq": "2.6.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-scripts": "3.0.1"
  },
  "devDependencies": {
    "nodemon": "1.18.4"
  },
  "keywords": [],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
